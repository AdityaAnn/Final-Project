class aClass {      // The class
  public:             // Access specifier
  int num;      // Attribute (int variable)
  string aSring;
  void combine (string this) {
    cout << this;     // Attribute (string variable)
};

int main() {
  
  // Create an object of NewClass
  aClass obj;
  
  // Access attributes and set values
  obj.num = 1;
  obj.aString = "This is a String";
  
  // Print attribute values
  cout << obj.num << "\n";
  cout << obj.aString <<"\n";
                      
  // Creating a new string
  string numAndString = obj.num + ‘ ’ + obj.aString;
                      
  // Calling combine function
  combine (numAndStrings);
  
  return 0;
}
